{
  "eslint.useFlatConfig": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "yaml"
  ],
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "testing.automaticallyOpenPeekView": "never",
  "typescript.tsdk": "lib/node_modules/typescript/lib",
  "versionlens.npm.onSaveChanges": "versionlens pnpm install"
}
